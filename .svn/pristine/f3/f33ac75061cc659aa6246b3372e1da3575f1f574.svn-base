            },
			showRightMsg(id){
                 const that = this;
                 $.ajax({
                     url: baseUrl + "/api/content/list",
                     data:{
                         "categoryId" : id,
                         "num" : "1",
                         "pageSize" : "10"
                     },
                     dataType:"json",
                     success:  function (result) {
                         that.newsRightList = result.data.list;
                         console.log(that.newsRightList);
                         console.log(result);
						var pageSi =Math.ceil(result.data.total/10);

                          $(".zxf_pagediv").createPage({
                              pageNum: pageSi,
                              current: 1,
                              backfun: function(e) {
                              that.pageContentId = e.current;
                              $.ajax({
