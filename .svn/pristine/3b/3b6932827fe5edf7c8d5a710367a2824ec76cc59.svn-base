				const that = this;
                that.isShow = true;
                 
                $.ajax({
                    type:"post",
                    url:unionUrl + "/passLaborweb/api/laborun/union/getLaborunListJsonPage",
                    contentType:'application/json;charset=UTF-8',
                    data:JSON.stringify({
                    	"laborunName":that.laborun_name,
