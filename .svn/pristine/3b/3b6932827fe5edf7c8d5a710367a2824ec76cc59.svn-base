<script>
    var vm = new Vue({
        el: "#nameselect",
        data: {
            isactive:false,
            userinp:'',
            userPhone:'',
            membercenter:'',
            exLaborunName:'', //原所属工会名称
            exUnitName:'', //原属单位
            unitName:'',//转入单位
            laborunName:'',//转入工会
            unionList:'',
            suoShuUnit:[],
            isShow:false,
            isShow2:false,
             iLoad:false,
            isLoad:false,
            unionName:[],
            unionDanWei:[],
            unioinNum:20,
            unionUNM:20,


            todayTime:'',
