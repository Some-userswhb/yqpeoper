            //选择工会
            showList(){
				const that = this;
                that.isShow = true;
                 
                $.ajax({
                    type:"post",
                    url:unionUrl + "/passLaborweb/api/laborun/union/getLaborunListJsonPage",
                    contentType:'application/json;charset=UTF-8',
                    data:JSON.stringify({
                        "laborunName":that.laborun_name,
                        "page":"1",
                        "size":"20",
                        "token":userTok.token
                    }),
                    async: false,
                    dataType:"json",
                    success:  function (result) {

                    	console.log(result.data.content);

                           that.unionList=result.data.content;
                        $("#union-name").scroll(function(){
                            var nScrollHight = 0; //滚动距离总长(注意不是滚动条的长度)
                            var nScrollTop = 0;   //滚动到的当前位置
                            var nDivHight = $("#union-name").height();

                            nScrollHight = $(this)[0].scrollHeight;
                            nScrollTop = $(this)[0].scrollTop;
                            // console.log(nScrollTop);
                            if(nScrollTop + nDivHight >= nScrollHight){
                                //显示加载更多
                                that.isLoad = true
							}else{
                                //隐藏加载更多
                                that.isLoad = false
							}
