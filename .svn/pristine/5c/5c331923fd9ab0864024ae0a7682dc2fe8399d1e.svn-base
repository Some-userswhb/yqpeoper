<script>
    var vm = new Vue({
        el: "#nameselect",
        data: {
            isactive:false,
            userinp:'',
            userPhone:'',
            unionList:'',
            suoShuUnit:'',
            MOhuyunion:'',
            MOhuyDan:'',

            uniondanList:'',
