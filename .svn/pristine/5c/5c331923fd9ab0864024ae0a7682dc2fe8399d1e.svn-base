			unionName:[],

			unionDanWei:[],
			start:0,
			endNum:50,
            isLoad:false,
            iLoad:false,
			arr:[],
            unioinNum:30,
            unionUNM:30,
            unionItem:'',
            suoShuItem:'',
            nameCode:'',
