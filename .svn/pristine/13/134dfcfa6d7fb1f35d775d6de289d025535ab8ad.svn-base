<script>
	// 声明变量
	var i=0;
	// 获取图片多少个
	var size=$("#img li").size();
	// 判断小点
	var str="";
	for(var j=0;j<size;j++){
		str+=' <li></li> ';
	}
	// 输入小点
	$("#num").html(str);
	// 默认选中第一个
	$("#num li").first().addClass('actives');
	var ding=null;
	// 定时器
	function move(){
		i++;
		// 如果图片超出最后一张，展示第一张图片
		if (i>=size) {
			i=0;
		};
		// 让对应编号的图片展示
		$("#img li").eq(i).addClass('active').siblings().removeClass('active');
		// 修改对应的小点
		$("#num li").eq(i).addClass('actives').siblings().removeClass('actives');
	}
	ding=setInterval(move,5000);
	// 给小点绑定点击事件
	$("#num li").mouseover(function(){
		clearInterval(ding);
		// 获取我点击的编号
		idx=$(this).index();
		// 对应改变图片和小点显示
		i=idx;
		// 让对应编号的图片展示
		$("#img li").eq(idx).addClass('active').siblings().removeClass('active');
		// 修改对应的小点
		$("#num li").eq(idx).addClass('actives').siblings().removeClass('actives');
	});
	// 鼠标离开
	$("#num li").mouseout(function(){
		ding=setInterval(move,5000);
	});
	// 右边点击事件
	$(".btn-right").click(function(){
		move();
	});
	// 左移
	$(".btn-left").click(function(){
		i--;
		if (i<0) {
			i=size-1;
		};
		// 让对应编号的图片展示
		$("#img li").eq(i).addClass('active').siblings().removeClass('active');
		// 修改对应的小点
		$("#num li").eq(i).addClass('actives').siblings().removeClass('actives');
	});
</script>
