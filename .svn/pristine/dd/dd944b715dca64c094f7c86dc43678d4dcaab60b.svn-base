}

.juxing{
    position: relative;
}


/*头部小方块样式*/
.wechat-mask {
    position: absolute;
    right: 75px;
    top: 155px;
}
.wechat-mask .jq{
    width: 520px;
    padding: 15px 20px;
    background: #fff;
    box-shadow: 0 0 7px rgba(0,0,0,0.36);
    position: absolute;
    top:0;
    right: 0;
}
.showhd{
    display: block !important;
}
.showmask2,.showmask1{
    z-index: 50;
    display: none;
}
.content,.xiala,.top-arrow2,.top-arrow2ccc,.top-arrow1,.top-arrow1ccc{
    display: none;
}
/*微信矩阵*/
.top-arrow2{
    width: 25px;
    height: 25px;
    background-color: white;
    box-shadow: -2px -2px 7px rgba(0,0,0,0.36);
    position: absolute;
    right: 39px;
    top: 102px;
    transform: rotate(45deg);
    z-index: 5;
}
.top-arrow2ccc{
    width: 25px;
    height: 25px;
    transform: rotate(45deg);
    position: absolute;
    right: 39px;
    top: 102px;
    background-color: #fff;
    z-index: 60;
}
.top-arrow1{
    width: 25px;
    height: 25px;
    background-color: white;
    box-shadow: -2px -2px 7px rgba(0,0,0,0.36);
    position: absolute;
    right: 39px;
    top: 102px;
    transform: rotate(45deg);
    z-index: 5;
}
.top-arrow1ccc{
    width: 25px;
    height: 25px;
    transform: rotate(45deg);
    position: absolute;
    right: 39px;
    top: 102px;
    background-color: #fff;
    z-index: 60;
}
/*结束*/

