body,html{margin: 0; padding: 0; font-family: "微软雅黑";}
ul,li,h1,h2,h3,h4,h5,h6,p{
	padding: 0;
	margin: 0;
	list-style: none;
}
a{text-decoration: none;}
.clearfix:after{
	content:"."; 
	display:block; 
	height:0; 
	visibility:hidden; 
	clear:both; 
}
.clearfix { zoom:1;}
