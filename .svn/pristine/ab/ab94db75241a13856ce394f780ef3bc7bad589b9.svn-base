/*复制首页小方块样式*/
.wechat-mask {
	position: absolute;
	right: 75px;
	top: 155px;
}
.wechat-mask .jq{
	width: 520px;
	padding: 15px 20px;
	background: #fff;
	box-shadow: 0 0 7px rgba(0,0,0,0.36);
	position: absolute;
	top:0;
	right: 0;
}
.showhd{
	display: block !important;
}
.showmask2,.showmask1{
	z-index: 50;
	display: none;
}
.content,.xiala,.top-arrow2,.top-arrow2ccc,.top-arrow1,.top-arrow1ccc{
	display: none;
}
.top-arrow2{
	width: 25px;
	height: 25px;
	background-color: white;
	box-shadow: -2px -2px 7px rgba(0,0,0,0.36);
	position: absolute;
	right: 170px;
	top: -13px;
	transform: rotate(45deg);
	z-index: 5;
}
.top-arrow2ccc{
	width: 25px;
	height: 25px;
	transform: rotate(45deg);
	position: absolute;
	right: 170px;
	top: -12px;
	background-color: #fff;
	z-index: 60;
}
.top-arrow1{
	width: 25px;
	height: 25px;
	background-color: white;
	box-shadow: -2px -2px 7px rgba(0,0,0,0.36);
	position: absolute;
	right: 35px;
	top: -13px;
	transform: rotate(45deg);
	z-index: 5;
}
.top-arrow1ccc{
	width: 25px;
	height: 25px;
	transform: rotate(45deg);
	position: absolute;
	right: 35px;
	top: -12px;
	background-color: #fff;
	z-index: 60;
}
.wechat-mask .jq_list{
	border-bottom: 1px solid #d7d7d7;
	padding: 10px 0;
}
.wechat-mask .jq_list ul{margin: 0;padding: 0;}
.wechat-mask .jq_list ul li{
	float: left;
	line-height: 35px;
	margin-right: 5px;
	text-align: center;
}
.wechat-mask .jq_list ul li a{
	font-size: 14px;
	color: #333333;
	padding: 5px 10px;
}
.wechat-mask .jq_list ul li.active a{background: #db2c28;color: #fff;padding: 5px 10px;}
.wechat-mask .jq_list ul li a:hover{background: #db2c28;color: #fff;padding: 5px 10px;}
.wechat-mask .jq_list ul li:first-child a{background: none;color: #db2c28;font-weight: bold;}






