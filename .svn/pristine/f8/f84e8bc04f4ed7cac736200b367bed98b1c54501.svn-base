.matter_title{
	width: 1200px;
	margin: 30px auto 0;
	border: 1px solid #cccccc;
	background: #fff;
	padding: 25px 0;
	position: relative;
}
.matter_title h4{
	font-size: 40px;
	text-align: center;
	font-weight: 700;
}
.matter_title h4 a{
	color: #222222;

}
.matter_title span{
	display: block;
	font-size: 16px;
	color: #666666;
	text-align: center;
}
.matter_more{
	position: absolute;
	top: 0;
	right: 20px;
}
.matter_more a{
	width: 100px;
	line-height: 36px;
	font-size: 14px;
	color: #fff;
	background: #db2c28;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	text-align: center;
	padding: 10px 20px;
}
.matter{
	width: 1200px;
	margin: 30px auto 0;
	overflow: hidden;
}
.matter_left{
	float: left;
	position: relative;
}
.matter_position{
	position: absolute;
	width: 100%;
	background: rgba(0,0,0,0.7);
	left: 0;
	bottom: 0;
	float: left;
}
.matter_position a{
	display: block;
	font-size: 16px;
	color: #fff;
	padding: 10px;
}
.matter_right{
	float: right;
	width: 49%;
}
.matter_list ul{
	width: 100%;
	height: 39px;
	overflow: hidden;
}
.matter_list ul li{
	float: left;
	width: 95px;
	margin-right: 20px;
}
.matter_list ul li a{
	display: block;
	font-size: 16px;
	color: #333333;
	line-height: 39px;
	text-align: center;
	font-weight: 700;
}
.matter_list ul li.active{background: #db2c28;}
.matter_list ul li.active a{color: #fff;}
.matter_list ul li:hover{background: #db2c28;}
.matter_list ul li:hover a{color: #fff;}
.shixian{
