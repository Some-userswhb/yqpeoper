.tan{
	position: absolute;
	bottom: 0;
	right: -403px;
	cursor: pointer;
	width: 400px;
	height: 250px;
	background: rgba(0,0,0,0.6);
	display: none;
	border-radius: 10px;
	margin-left: 10px;
}
.ty_left h4{
	text-align: center;
	font-size: 24px;
	color: #fff;
	margin: 10px 0 30px;
}
.ty_left a img{
	display: block;
	margin: 0 auto;
}
.ty_left>ul{
	margin-top: 30px;
	width: 160px;
	height: 68px;
	overflow: hidden;
}
.ty_left>ul>li{
	float: left;
	margin-right: 15px;
	font-size: 16px;
	color: white;
	cursor: pointer;
	
}
.ty_left>ul>li>a{
	font-size:14px;
	color: #fff;
	line-height: 32px;
}
.tan ul{
	width: 90%;
	margin: 0 auto;
	margin-top: 10px;
	height: 224px;
	overflow: hidden;
	overflow-y: auto;
}
.tan ul::-webkit-scrollbar {/*滚动条整体样式*/
    width: 4px;     /*高宽分别对应横竖滚动条的尺寸*/
    height: 4px;
}
.tan ul::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
    border-radius: 5px;
    -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
    background: rgba(0,0,0,0.6);
}
.tan ul::-webkit-scrollbar-track {/*滚动条里面轨道*/
    -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
    border-radius: 0;
    background: #fff;
}

.tan ul li{
	float: left;
	color: #fff;
	margin-bottom: 10px;
	margin-right: 10px;
	font-size: 15px;
	padding: 5px 10px;
	background: #db2c28;
	border-radius: 5px;
}
