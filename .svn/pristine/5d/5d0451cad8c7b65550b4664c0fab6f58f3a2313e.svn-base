<script>
    $(function () {

        /*
         *  1.要点击的元素 类名
         *	2.要切换的元素 tab
         * */
        var heig = $('.tab-togg').height();
        $('.my_ghcontent').css('height',heig)

        $(".btn").on("click",function(){
            var dd=$(".erji").height();
            // console.log(dd)
            $('.my_ghcontent').css('height',dd)
        })
        // var heig = $('.layer-table').height();
        // $('.layer-table').css('height',heig)


        function tabtogg(clicClass,togclass,clica,clici) {
            $(clicClass).click(function () {
                $('.tab-togg').removeClass('on')
                $(togclass).addClass('on')
                var hei = $(togclass).height();
                $('.my_ghcontent').css('height',hei)
                $('.rh_list li p').removeClass('bordred')
                $(clica).addClass('bordred')
                $('.rh_list li i').css('color','#000')
                $(clici).css('color','#db2c28')
            })

        }



        tabtogg('.securitySettings','.my_personalinfo','.securitySettings p','.securitySettings i')
        tabtogg('.personalinfo','.shiming','.personalinfo p','.personalinfo i')
        tabtogg('.ChairmanMailbox','.my-yuanzhu','.ChairmanMailbox p','.ChairmanMailbox i')
        tabtogg('.Lawyerservice','.layer-ruzhu','.Lawyerservice p','.Lawyerservice i')
        tabtogg('.Caringhotline','.my-hotline','.Caringhotline p','.Caringhotline i')
        tabtogg('.Myanswer','.my-answer','.Myanswer p','.Myanswer i')
        // tabtogg('.renwu_list','.xinxiang','.renwu_list p','.renwu_list i')




        tabtogg('.my-union','.my-enterUnion','.my-union p','.my-union i')
        tabtogg('.my-transfer','.my-turnUnion','.my-transfer p','.my-transfer i')
        tabtogg('.my-tiao','.my-tiaojie','.my-tiao p','.my-tiao i')
        tabtogg('.my-dai','.my-daishu','.my-dai p','.my-dai i')
        tabtogg('.my-over','.renwu_over','.my-over p','.my-over i')
        tabtogg('.my-wei','.renwu_myover','.my-wei p','.my-wei i')
        tabtogg('.unit-crate','.unit-building','.unit-crate p','.unit-crate i')
        tabtogg('.legal-person','.legal-content','.legal-person p','.legal-person i')
        tabtogg('.my-letter','.my-letterbox','.my-letter p','.my-letter i')
        // tabtogg('.securitySettings ','.my-enterUnion','.securitySettings  p','.securitySettings  i')
        tabtogg('.my-renwuover','.my-overcountt','.my-renwuover p','.my-renwuover i')
        tabtogg('.my-renwuwei','.my-weicountt','.my-renwuwei p','.my-renwuwei i')


        $('.same').click(function () {
            $(this).next().addClass("hei").prev().siblings().removeClass("hei");
            $(this).find("i").addClass("rotate").parent().siblings().find("i").removeClass("rotate");
            if($(this).next().hasClass('hei')){
                // alert(111);
                // $(this).prev().removeClass('active');
                $(this).next().removeClass('hei')
                $(this).find("i").removeClass('rotate')

            }else{
                // $(this).prev().addClass('active');
                $(this).next().addClass('hei')
                $(this).find("i").addClass('rotate')
            }
        })
        $('.layer-btn button').click(function(){
            $('.rh_boxed').css('display','none');
            $('.layer-table').css('display','block');
        })

    })
    // 援助案例、律师说法切换
    $(".layer-button>div").on("click",function(){
        $(this).addClass("actived").siblings().removeClass("actived");
    })
</script> <!-- 改成你的路径 -->
