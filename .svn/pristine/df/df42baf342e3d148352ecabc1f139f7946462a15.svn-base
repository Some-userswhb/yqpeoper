        methods:{
            //遮罩层1左侧菜单列表
            getMask1LeftMenu(){
                const that = this;
                $.ajax({
                    url: baseUrl + "/api/category/list",
                    data:{
                        parentId:"290"
                    },
                    async: false,
                    dataType:"json",
                    success:  function (result) {
                        that.mask1left = (result.data);

                    }
                })
            },
            getMask1LeftContent(){
                const that = this;
                $.ajax({
                    url: baseUrl + "/api/category/list",
                    data:{
                        parentId:"290"
                    },
