                        "categoryId" : id,
                        "num" : "1",
                        "pageSize" : "10"
                    },
                    dataType:"json",
                    success:  function (result) {
                        that.mask1right = result.data.list;
                        console.log(that.mask1right)

                    }
                })
            },

            gotoSer(path){
                window.open(path,'_self')
			},
