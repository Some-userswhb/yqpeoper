<script src="js/usertoken.js"></script>
<script>
	var vm = new Vue({
		el:'#wrap',
		data:{
            username:'',
			password:'',
            userinp:'北京',
		},
		methods:{

			login(){
                    var that = this
				    if(that.username.length != 11){
				    	 $('#myModal').modal();
					     $('.mm_cz>span').html('请输入11位手机号码~~');
						 $(".dl_anniu").hide();
					}else if(!(/^1[34578]\d{9}$/.test(that.username))){
                         $('#myModal').modal();
					     $('.mm_cz>span').html('手机号格式不正确！');
					     $(".dl_anniu").hide();
					}else{
						
                        $.ajax({
                            url: unionUrl + "/sso/ssoUser/login/phone",

                            data: {
                                "phone": that.username,
                                "password": that.password,
                                "systemCode": 'S1003',
                                "source":'3',
                            },
                            async:true,
                            dataType: "json",
                            success: function (result) {

                                if(result.code == 0){
                                    console.log(result)
                                    var userId = result.data.token +'-'+
										result.data.user.phone +'-'+
										result.data.user.realName

									 sessionStorage.setItem('tok',userId);
									 var str  = sessionStorage.getItem('tok');
									 console.log(str)

									window.open('index.html','_self')

                                }else{


									if(result.msg === '电话号码不存在！'){
										// $('#myModal2').css('display','block')
										$('#myModal').modal();
					                    $('.mm_cz>span').html('您尚未注册！');
					                    $(".dl_anniu").show();
									}else{
                                        $('#myModal').modal();
					                    $('.mm_cz>span').html(result.msg);
					                    $(".dl_anniu").hide();
					                    that.password = ''

									}
                                }



                            }
                        })



					}


				},
				btnZhuce(){
					window.open("zhuce.html","_self");
				},

            sendModel(){
                kfhuifu();
			},
            goToSer(path){
                window.open(path,'_self')
            },
		},
		mounted(){

		}
	})
</script>


