<script>
    $(function () {

		/*微信矩阵 网站集群显示隐藏 复制*/
        $('#showmask1').mouseenter(function () {
            $('.showmask2').show();
            $('.top-arrow2').show()
            $('.top-arrow2ccc').show()
            $('.showmask1').hide()
            $('.top-arrow1').hide()
            $('.top-arrow1ccc').hide()
        });
        $('.showmask2').mouseleave(function () {
            $('.showmask2').hide()
            $('.top-arrow2').hide()
            $('.top-arrow2ccc').hide()
        });
        $('#showmask2').mouseenter(function () {
            $('.showmask1').show()
            $('.top-arrow1').show()
            $('.top-arrow1ccc').show()
            $('.showmask2').hide()
            $('.top-arrow2').hide()
            $('.top-arrow2ccc').hide()
        });
        $('.showmask1').mouseleave(function () {
            $('.showmask1').hide()
            $('.top-arrow1').hide()
            $('.top-arrow1ccc').hide()
        });


        $('.hideMask').mouseenter(function () {
            $('.xiala').hide();
            $('.content').hide()
        })



        addactive('.list_online .list ul li','active');
        addactive('.slide ul li','active');
        addactive('.menu ul li','mu1');

        $('.fk_num ul li').click(function () {
            $(this).addClass('video-active').siblings().removeClass('video-active');

        })

    })
</script>
