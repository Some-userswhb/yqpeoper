<script>

    var number = 50;


    var obj = {
        number:60,
        getNum:function () {
            var number = 70;
            return this.number
        }
    }

    //跟两个var 没有关系   直接调用这个函数 this指向当前obj  obj.number = 60
    console.log(obj.getNum())     //60

    //call 改变this指向
    console.log(obj.getNum.call()); //50
    //指向当前括号里面的对象   所以=20
    console.log(obj.getNum.call({number:20}))   //20


</script>

