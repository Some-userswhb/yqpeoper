<script>
    $(function () {
    	
   
		/*微信矩阵 网站集群*/
        $('#showmask2').mouseenter(function () {
            $('.showmask2').show();
            $('.top-arrow2').show()
            $('.top-arrow2ccc').show()
            $('.showmask1').hide()
            $('.top-arrow1').hide()
            $('.top-arrow1ccc').hide()

        });
        $('.showmask2').mouseleave(function () {
            $('.showmask2').hide()
            $('.top-arrow2').hide()
            $('.top-arrow2ccc').hide()
        });
        $('#showmask1').mouseenter(function () {
            $('.showmask1').show()
            $('.top-arrow1').show()
            $('.top-arrow1ccc').show()
            $('.showmask2').hide()
            $('.top-arrow2').hide()
            $('.top-arrow2ccc').hide()
        });
        $('.showmask1').mouseleave(function () {
            $('.showmask1').hide()
            $('.top-arrow1').hide()
            $('.top-arrow1ccc').hide()
        });

        $('.hideMask').mouseover(function () {

            $('.xiala').hide();
            $('.content').hide()

        })
		$('.ceshi').click(function(){
			$('.nologin-maskbox').css('display','block');
			$('.nologin-mask').css('top','0px')

		 })
		$('.nologin-title i').click(function(){
			$('.nologin-maskbox').hide();
             $('.nologin-mask').css('top','-200px')

             $('.nologin-maskbox').css('display','none');
		 })
		$('.nologin-close input').click(function(){
			$('.nologin-maskbox').hide();
             $('.nologin-mask').css('top','-200px')
             $('.nologin-maskbox').css('display','none');


         })
		/*搜索框上 导航列表*/
        function addactive(classdom, classname) {
            $(classdom).click(function () {
                $(classdom).removeClass(classname)
                $(this).addClass(classname)
            });
        }

        addactive('.list ul li', 'active')

		/*组织机构 新闻资讯*/
        $('.tissue-jg').mouseover(function () {
            $('.xiala').show();
            $('.content').hide()
        });
        $('.content').mouseleave(function () {
            $('.xiala').hide()
            $('.content').hide()
        });
        $('.news-zx').mouseover(function () {
            $('.content').show();
            $('.xiala').hide()
        });
        $('.xiala').mouseleave(function () {
            $('.content').hide()
            $('.xiala').hide()
        })

        //微信二维码
        $('.slide ul li:nth-child(5)').mouseover(function () {
            $('.ewm').show();
        });
        $('.slide ul li:nth-child(5)').mouseleave(function () {
            $('.ewm').hide();
        });


        $('.slide li').click(function () {
            $('.slide li').removeClass('active')
            $(this).addClass('active')
        })
        //搜索
        $('.search').click(function () {
            window.open('ssjg.html','_self')
        })
        
		$('.app-android').css('right','0px');
		$('.app-android').css('opacity','1');
		$('.app-ios').css('left','0px');
		$('.app-ios').css('opacity','1');

    })
</script>
