
/*头部样式*/
.nav1{
	width: 1200px;
	margin: 0 auto;
	overflow: hidden;
	padding: 40px 0;
}
.nav_left{
	float: left;
}
.nav_center{
	float: left;
	margin: 0 70px;
}
.nav_right{
	float: right;
}
.juxing{
	float: left;
	background: #db2c28;
	width: 100px;
	height: 90px;
	border-radius: 5px;
	text-align: center;
	cursor: pointer;
}
.juxing i{
	display: block;
	font-size: 38px;
	color: #fff;
}
.juxing i{color: #fff;}
.juxing p{
	font-size: 16px;
	color: #fff;
}
.pic{margin-left: 30px;}
.list{
	margin: 0 auto;
}
.list ul{margin: 0; padding: 0;}
.list ul li{
	cursor: pointer;
	overflow: hidden;
	float: left;
	width: 90px;
	text-align: center;
	margin-right: 13px;
}
.list ul li a{
	font-size: 15px;
	color: #666666;
	line-height: 34px;
	margin-left: 5px;
}
.list ul li i{
	font-size: 16px;
	line-height: 34px;
}
.list ul li i{color: #666666;}
.list ul li a{color: #666666;}
.list ul li:hover{
	background: #db2c28;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
}
.list ul li:hover a{color: #fff;}
.list ul li:hover i{color: #fff;}
.list ul li.active{
	background: #db2c28;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
}
.list ul li.active i{color: #fff;}
.list ul li.active a{color: #fff;}
.search{
	margin: 0 auto;
}
.text{
	width:400px;
	height:40px;
	border: 1px solid #db2c28;
	float: left;
	border-right: none;
	font-size: 14px;
	color: #999999;
	padding-left: 20px;
}
.button{
	width: 70px;
	height: 40px;
	border: 1px solid #db2c28;
	float: left;
	background: #db2c28;
	font-size: 16px;
	color: #fff;
}
.search_text{
	margin: 10px auto 0;
}
.search_text ul li{
	float: left;
	width: 70px;
}
.search_text ul li a{
	font-size: 14px;
	color: #474747;
}
.search_text ul li a:hover{color: #db2c28;}
.menu{
	width: 1200px;
	margin: 135px auto 0;
}
.menu ul{margin: 0;padding: 0;}
.menu ul li{
	float: left;
	width: 150px;
	padding: 15px 0;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	border-bottom: 3px solid #db2c28;
}
.menu ul li i{
	display: block;
	font-size: 36px;
	color: #db2c28;
	text-align: center;
}
.menu ul li a{
	display: block;
	line-height: 28px;
	text-align: center;
	font-size: 20px;
	color: #db2c28;
}
.menu ul li.mu1{background: #db2c28;}
.menu ul li.mu1 i{color: #fff;}
.menu ul li:hover i{color: #fff; }
.menu ul li:hover{background: #db2c28;}
.menu ul li:hover a{color: #fff;}
.menu ul li.mu1 a{color: #fff;}
.menu ul li:hover i{color: #fff;}
.my_ghtitleleft{
	float: left;
	margin-top: 30px;
}
.my_ghtitleleft i{
	font-size: 22px;
	color: #db2c28;
}
.my_ghtitleleft a{
	font-size: 14px;
	color: #333333;
}
.my_ghtitleleft span{
	font-size: 14px;
	color: #db2c28;
	font-weight: bold;
}





.dl_navbox{
	width: 100%;
	background: url(../images/dl_nav.png) top center no-repeat;
}
.dl_nav{
	width: 1200px;
	margin: 0 auto;
}

.dl_nav ul li{
	padding:0 36px;
	float: left;

}
.dl_nav ul li a{
	display: block;
	width: 100%;
	height: 100%;
	font-size: 20px;
	color: #fff;
	line-height: 60px;
	text-align: center;
}
.dl_bigbox{
	width: 100%;
	padding: 50px 0;
	background: url(../images/dl_bg.png) center no-repeat;
}
.dl_box{
	width: 1200px;
	margin: 0 auto;
	background: rgba(255,255,255,0.36);
}
.dl{
	padding-top: 40px;
}
.dl_img{
	float: left;
	width: 600px;
	padding-left: 50px;
}
.dl_title{
	float: left;
	margin-left: 120px;
	position: relative;
}
.dl_title:after{
	content: '';
	width: 96px;
	height: 2px;
	background: #db2c28;
	position: absolute;
	left: 20px;
	top: 64px;
}
.dl_title:before{
	content: '';
	width: 96px;
	height: 2px;
	background: #db2c28;
	position: absolute;
	right: 20px;
	top: 64px;
}
.dl_title h4{
	font-size: 25px;
	color: #db2c28;
	text-align: center;
	letter-spacing:8px;
	margin-top: 50px;
}
.hengxian{
	width: 1px;
	height: 415px;
	background: #bcbcbc;
	margin: 0 auto;
}
.dl_input{
	position: relative;
}
.dl_input input{
	display: block;
	width: 360px;
	height: 50px;
	background: #e5e5e5;
	border: none;
	border-radius: 5px;
	padding-left: 50px;
	font-size: 16px;
	color: #333333;
	margin-bottom: 20px;
}
.ziti{
	position: absolute;
	left: 15px;
	top: 10px;
}
.ziti i{
	font-size: 22px;
	color: #868686;
}
.dl_btn{
	width: 360px;
	height: 50px;
	background: #db2c28;
	border: none;
	border-radius: 5px;
	font-size: 18px;
	color: #fff;
}
.wx{
	cursor: pointer;
	float: left;
	margin-top: 20px;
}
.wx i{
	font-size: 22px;
	color: #51bc79;
}
.wx a{
	font-size: 14px;
	color: #333333;
	padding-left: 10px;
}
.mm{
	float: right;
	margin-top: 25px;
}
.mm span{
	font-size: 14px;
	color: #333333;	
}
.mm a{
	font-size: 14px;
	color: #333333;	
	padding-left: 20px;
}
