<script>
    $(function () {

		/*微信矩阵 网站集群显示隐藏 复制*/
        $('#showmask1').mouseenter(function () {
            $('.showmask2').show();
            $('.top-arrow2').show()
            $('.top-arrow2ccc').show()
            $('.showmask1').hide()
            $('.top-arrow1').hide()
            $('.top-arrow1ccc').hide()
        });
        $('.showmask2').mouseleave(function () {
            $('.showmask2').hide()
            $('.top-arrow2').hide()
            $('.top-arrow2ccc').hide()
        });
        $('#showmask2').mouseenter(function () {
            $('.showmask1').show()
            $('.top-arrow1').show()
            $('.top-arrow1ccc').show()
            $('.showmask2').hide()
            $('.top-arrow2').hide()
            $('.top-arrow2ccc').hide()
        });
        $('.showmask1').mouseleave(function () {
            $('.showmask1').hide()
            $('.top-arrow1').hide()
            $('.top-arrow1ccc').hide()
        });


        //字体放大 缩小 正常
        var  num = 16;
        $('.sizeBig').click(function () {
            num += 2;
            if(num > 26){
                num = 26;
                alert('当前字体已经是最大');
            }
            $('.tt_text  p').css('font-size', num + 'px')
            $('.tt_datails  p').css('font-size', num + 'px')


        });

        $('.sizeNormal').click(function () {
            num = 16;
            $('.tt_text p').css('font-size',  num+ 'px');
            $('.tt_datails p').css('font-size',  num+ 'px');

        });

        $('.sizeSmall').click(function () {
            num-=2;
            if(num < 12){
                num = 12

                alert('当前已经是最小字体了');
            }
            $('.tt_text  p').css('font-size', num + 'px');
            $('.tt_datails  p').css('font-size', num + 'px');

        });


        addactive('.list_online .list ul li','active');
        addactive('.home_list li','active');
        addactive('.matter_list ul li','active');
        addactive('.slide ul li','active');
        addactive('.menu ul li','mu1');





		/*组织机构 新闻资讯*/
        $('.tissue-jg').mouseover(function () {

            $('.xiala').show();
            $('.content').hide()
        });
        $('.content').mouseleave(function () {
            $('.xiala').hide();
            $('.content').hide();
        });
        $('.news-zx').mouseover(function () {
            $('.content').show();
            $('.xiala').hide();
        });
        $('.xiala').mouseleave(function () {
            $('.content').hide();
            $('.xiala').hide();
        })

        $('.hideMask').mouseenter(function () {

            $('.xiala').hide();
            $('.content').hide()

        })



    })
</script>
