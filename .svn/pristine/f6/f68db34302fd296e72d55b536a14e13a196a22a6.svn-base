body,html{margin: 0; padding: 0; font-family: "微软雅黑";}
ul,li,h1,h2,h3,h4,h5,h6,p{
	padding: 0;
	margin: 0;
	list-style: none;
}
a{text-decoration:none !important;}
.zg_box{
	width:100%;
	background: url(../images/banner1.png) top center no-repeat;
}

.bordred{
	color: #db2c28 !important;
	border-left: 3px solid #db2c28 !important;
}
.tab-togg{
	display: none;
	position: absolute;
	background-color: #fff;
	left: 30%;

}
.on{
	display: block;
}


/*幸福女工*/
.female-content{
	width: 845px;
}
 .female-work{
	border-bottom: 1px dashed #ccc;
	margin-bottom: 20px;
}{
	border-bottom: 1px dashed #ccc;
	margin-bottom: 20px;
}
 .female-work .female-img{
	width: 28%;
	height: 210px;
	float: left;
}
 .female-work .female-img img{
	width: 100%;
	height: 100%;
	margin-bottom: 20px;
}
 .female-work .female-content{
	width: 67%;
	float: left;
	margin-left: 20px;
	margin-bottom: 20px;
}
 .female-work .female-content p:first-child{
	font-size: 20px;
	font-weight: bold;
	margin-bottom: 25px;
}
.female-work .female-content .fir-content{
	margin-bottom: 25px;
	height: 110px;
	overflow: hidden;
