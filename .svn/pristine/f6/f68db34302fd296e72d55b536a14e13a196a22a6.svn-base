.jwn_list{
	margin-top: 45px;
	margin-left: 260px;
}

.jwn_list ul li{
	float: left;
	margin-right: 20px;
}
.jwn_list ul li a{
	font-size: 14px;
	color: #666666;
	padding: 5px 10px;
	border: 1px solid #cccccc;
}
.jwn_list ul li:hover a{
	color: #fff;
	background: #db2c28;
	padding: 5px 10px;
	text-decoration: none;
	border: 1px solid #db2c28;
}
.jwn_list ul li.active a{
	color: #fff;
	background: #db2c28;
	border: 1px solid #db2c28;
}


















