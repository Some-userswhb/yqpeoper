<script>
    var vm = new Vue({
        el: "#nameselect",
        data: {
            isactive:false,
            userinp:'',
            userPhone:'',
            membercenter:'',


			unionName:'',//所属工会
			personName:'',//法人姓名
			number:'',//联系电话


        },
        methods:{

            sendModel(){
                kfhuifu();
            },
             clear(){
            	  sessionStorage.clear();
            	  window.open("denglu.html","_self")
            },
            addjiao(){

            	var that = this;
            	//法人资格
                $.ajax({
                    // 请求方式
                    type:"post",
                    dataType:"json",
                    // url
                    contentType:'application/json;charset=UTF-8',
                    url: unionUrl + '/passLaborweb/api/laborun/laborunLegal/addApp',
                    // 把JS的对象或数组序列化一个json 字符串
                    data:JSON.stringify({
                        "laborunName":that.unionName,
                        "applyName":that.personName,
                        "applyPhone":that.number,
                        "source":"3",
                        "token": userTok.token,
                    }),
                    // result 为请求的返回结果对象
                    success:function (result) {
                    	  $('#myModal').modal();
                          $('.mm_cz>span').html(result.msg);
                        console.log( result)  //成功
                        if(result.msg == "success"){
                        	$(".same_list li:nth-child(1)").addClass("act");
                        	$(".same_list li:nth-child(2)").addClass("act");
                        	$(".same_list li:nth-child(3)").addClass("act");
                        	$(".same_list li:nth-child(4)").addClass("act");
                        	$(".same_list li:nth-child(5)").addClass("act");
                        	$(".same_list li:nth-child(6)").addClass("act");
                        	$(".same_list li:nth-child(7)").addClass("act");
                        }

						if(result.code === "0"){
							$('#myModal').modal();
                           $('.mm_cz>span').html('法人资格已添加');
                            // alert('法人资格已添加')
						}else{
                            // alert(result.msg);
                            if (result.msg=="参数错误") {
                            	 $('#myModal').modal();
                              $('.mm_cz>span').html("输入错误，请重试！");
                            }
                             

                            that.unionName = '';
                            that.personName= '';
                            that.number= ''
						}

                    }
                });

            }

        },
        mounted(){

            const that = this;
            if(userTok.token == null){
                this.membercenter = '请登录';
                $('.slide ul li:nth-child(2)').hide();

                //跳转登录界面 去登录
                $('.open-hyzx').click(function () {
                    window.open('denglu.html')
                });

            }else{
                this.membercenter = '会员中心';
                this.userPhone = userTok.phone;
                $('.slide ul li:nth-child(2)').hide();
                tuilogin()
                //登录成功 跳转会员中心
                $('.open-hyzx').click(function () {
                    window.open('hyzx.html')
                });

            }

        }



    })


</script>
