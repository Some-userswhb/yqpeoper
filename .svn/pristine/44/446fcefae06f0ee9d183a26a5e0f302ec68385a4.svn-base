/*所属工会*/
 .shu-union{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: rgba(0,0,0,0.7);
	display: block;
	overflow: hidden;
}
.shu-unionnei{
	width: 500px;
	height: 400px;
	background: #fff;
	margin: 130px auto;
	border-radius: 5px;
}
 .union-title, .unit-title{
	width: 100%;
	height: 50px;
	line-height: 50px;
	text-align: left;
	border-bottom: 1px solid #ccc;
	padding-left: 10px;
	color: #999;
}
.union-title p {
	float: left;
	width: 20%;
}
#sousuo,#sercer{
	float: left;
	width: 44%;
	height: 35px;
	border: none;
	outline: none;
	border-radius: 5px;
	background-color: #ccc;
	margin-right: 20px;
	padding-left: 15px;
	margin-left: 15px;
}

.content-small-title button{
	display: block;
	width: 60px;
	height: 35px;
	background: red;
	color: #fff;
	border: none;
	border-radius: 6px;
}
 .union-content{
	 width: 100%;
	 height: 85%;
 }
 .content-small-title{
	width: 100%;
	height: 35px;
	margin-top: 10px;

 }
.content-small-title .p{
	float: left;
	width: 30%;
	height: 35px;
	line-height: 35px;
	border-radius: 6px;
	background-color: red;
	color: white;
	padding-left: 20px;
	margin-left: 20px;
	border: none;

}
.union-content ul li{
	width: 45%;
	text-align: center;
	height: 40px;
	line-height: 40px;
	float: left;
	cursor: pointer;
	border-radius: 6px;
	font-size: 17px;
	font-weight: bold;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.union-content ul li:hover{
	background-color: #999;
	color: white;
}
/*所属单位*/
.sq_content .shu-unit{
	border: 1px solid #ccc;
	width: 550px;
	height: 300px;
	margin-left: 196px;
}





