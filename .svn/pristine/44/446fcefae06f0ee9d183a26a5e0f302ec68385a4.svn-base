.sp_title h4{
	font-size: 28px;
	color: #404040;
	text-align: center;
	font-weight: bold;
}
.sp_list{
	margin:60px 0 80px 0;
}
.sp_list ul li{
	float: left;
	width: 100px;
	margin: 0 10px;
}
.sp_list ul li.active i{color: #fff;background: #db2c28;}
.sp_list ul li.next.active p{color: #db2c28;}
.sp_list ul li a{text-decoration: none;}
.sp_tubiao i{
	font-size: 70px;
	color: #e5e2e2;
}
.sp_img{text-align: center;}
.sp_img i{
	display: block;
