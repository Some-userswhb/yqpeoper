	background: #e5e2e2;
	border-radius: 50%;
	padding: 30px;
}
.sp_text p{
	text-align: center;
	font-size: 16px;
	color: #333333;
	width: 170px;
	margin-top: 15px;
	margin-left: -30px;
}
.sq{
	margin-top: 70px;
	text-align: center;
}
.sq_list h4{
	font-size: 28px;
	color: #404040;
	text-align: center;
	font-weight: bold;
}
.sq_content{margin-top: 40px;overflow: hidden;}
.sq_content a{
	float: left;
	width: 200px;
	text-align: right;
	font-size: 16px;
	color: #333333;
	margin-top: 10px;
}
.sq_content i{
	font-style: normal;
	color: #db2c28;
	margin-right: 5px;
}
.sq_content a:hover{text-decoration: none;}
.sq_content input{
	float: left;
	color: #999999;
	font-size: 14px;
	width: 500px;
	height: 40px;
	border: none;
	border: 1px solid #cccccc;
	padding-left: 10px;
}
.sq_content select{
	float: left;
	width: 500px;
	height: 40px;
	border: 1px solid #cccccc;
	color: #999999;
	padding-left: 10px;
	appearance:none;
 	-moz-appearance:none; 
 	-webkit-appearance:none; 
    background: url(../images/select.png) 470px center no-repeat;
}
