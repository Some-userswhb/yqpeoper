.slide{
	position: fixed;
	top: 150px;
	right: 0;
	background: #fff;
}
.slide ul li:last-child{border: 1px solid #cccccc;}
.slide ul li.active{background: #db2c28;}
.slide ul li.active i{color: #fff;}
.slide ul li.active a{color: #fff;}
.slide ul li i{
	display: block;
	font-size: 28px;
	text-align: center;
	color: #db2c28;
}
.slide ul li a{
	display: block;
	font-size: 14px;
	text-align: center;
	color: #666666;
}
