<script>
    var vm = new Vue({
        el: "#nameselect",
        data: {
            phone:'',
            userinp:'',
			userPhone:'',

            humanFace:'',

			userName:'',
			nationality:'',
			birthday:'',

			renpic:'',
			IDzhengpic:'',
			IDfanpic:'',
            membercenter:'',
            nowTime:'',
            nowDay:'',
            userHeaderPic:'images/user.png'
        },
        methods:{

            //修改密码
            changepassword(){
            	
                var that = this;
                if(that.yespassword != that.newpassword){
                    alert('两次输入的密码不一致 请您重新输入')
                    that.yespassword = '';
                    that.newpassword = '';
				}else{

                    var chanpass = location.search
					var token = chanpass.substring(1)
					console.log(token)
                    $.ajax({
                        url: "http://112.35.92.177:90/sso/ssoUser/updatePassword",
                        data: {
                            "password":that.phone,
                            "newPassword": that.oldpassword,
							"token":token
                        },
                        async:true,
                        dataType: "json",
                        success: function (result) {

							alert(result.msg)
                        }
                    })
				}




            },
            sendModel(){
                kfhuifu();
			},
			clear(){
            	  sessionStorage.clear();
            	  window.open("denglu.html","_self")
            },



        },
        mounted(){

            const that = this;
            that.nowTime = nowTimeDay

            getDay()
            that.nowDay = nowDay



            //实名制查询   显示图片 名字
            $.ajax({
                type: 'post',
                url: unionUrl + "/passLaborweb/api/laborun/realnameRecord/queryRealnameRecord",
                contentType: 'application/json',
                data: JSON.stringify({
                    "token": userTok.token
                }),
                async: false,
                success: function (result) {
                    console.log(result);
                    that.userHeaderPic = result.data.content[0].portraitPic

                }

            })



        }



    })


</script>
